{
  "terminal.integrated.defaultProfile.windows": "Ubuntu-18.04 (WSL)",
    "editor.tabSize": 2,
    "editor.detectIndentation": false,
    "editor.quickSuggestions": {
      "strings": true
    },
    "git.ignoreMissingGitWarning": true,
    "editor.minimap.enabled": false,
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.includeLanguages": {
      "javascript": "javascriptreact",
      "erb": "html"
    },
    "tailwindCSS.includeLanguages": {
      "javascript": "javascript",
      "html": "HTML",
      "plaintext": "javascript"
    },
    "cSpell.userWords": [
      "preconnect"
    ],
    "launch": {


      "version": "0.2.0",
      "configurations": [
          {
              "type": "node",
              "request": "launch",
              "name": "Launch Local Node File",
              // "console": "integratedTerminal",
              "console": "internalConsole",
              "program": "${file}"
          },
          {
              "name": "Launch index.html",
              "type": "chrome",
              "request": "launch",
              "file": "${workspaceFolder}/index.html"
          },
      ]
  },
  "liveServer.settings.CustomBrowser": "chrome",
  "liveServer.settings.useLocalIp": true,
  "workbench.iconTheme": "Monokai Pro (Filter Spectrum) Icons",
  "liveServer.settings.NoBrowser": false,
  "[ruby]": {

  },
  "editor.fontFamily": "'Cascadia Code', Consolas, 'Courier New', monospace",
  "editor.fontWeight": "350",
  "vsicons.dontShowNewVersionMessage": true,
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "security.workspace.trust.untrustedFiles": "open",
  "css.validate": false,
  "editor.fontSize": 13,
  "calva.highlight.rainbowIndentGuides": true,
  "workbench.colorTheme": "Monokai Pro (Filter Spectrum)",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.tokenColorCustomizations": {
  "[Rubber]": {

  }
  },
  "workbench.startupEditor": "none",
  "java.showBuildStatusOnStart.enabled": "notification",
  "[python]": {
    "editor.formatOnType": true
  },
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "clojure": true,
    "javascript": true
  },
  "calva.paredit.defaultKeyMap": "strict",
  "window.zoomLevel": 1,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "workbench.sideBar.location": "right",
  "calva.legacyPrintBareReplWindowOutput": true,
  "diffEditor.codeLens": true
}
