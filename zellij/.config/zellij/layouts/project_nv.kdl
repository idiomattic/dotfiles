layout {
    default_tab_template {
        children
        pane size=1 borderless=true {
            plugin location="zjstatus"
        }
    }

    tab name="default" {
        pane split_direction="horizontal" {
            pane size="70%" name="agent"
            pane size="30%" focus=true name="misc"
        }
    }
    tab name="editor" {
        pane {
            command "nvim"
        }
    }
}
